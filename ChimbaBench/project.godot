; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="ChimbaBench"
run/main_scene="res://ChimbaBench.tscn"
run/disable_stdout=true
boot_splash/show_image=false
config/icon="res://icon.png"

[compression]

formats/zstd/long_distance_matching=true
formats/zstd/compression_level=16

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false
window/vsync/use_vsync=false

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom="res://Default_theme.tres"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/fallback_to_gles2=true
misc/lossless_compression/webp_compression_level=4
quality/shadow_atlas/cubemap_size=1024
quality/shadows/filter_mode=2
quality/voxel_cone_tracing/high_quality=true
